<p id="notice"><%= notice %></p>

<h1>Bookmarks</h1>
<div id="message"></div>
<div id="addBookmark">
  <%= render 'form', bookmark: @bookmark %>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Category</th>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="indexList">
    <%= render 'index' %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookmark', new_bookmark_path %>
