<p id="notice"><%= notice %></p>

<div id="message"></div>
<div id="addBookmark">
  <%= render 'form', bookmark: @bookmark %>
</div>

<h1 class="my-3">Your Bookmarks</h1>
<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Category</th>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="indexList">
    <%= render 'index' %>
  </tbody>
</table>
